{
  "name": "beep-it",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "gameserver": "tsx src/app/api/server.ts",
    "gameserver-forever": "forever start -c 'npm run gameserver' .",
    "_dev": "next dev --turbopack --port $BEEPIT_PORT",
    "dev": "dotenv-run-script -- _dev",
    "dev-forever": "forever start -c 'npm run dev' .",
    "prestart": "cp readme.md public/readme.md",
    "build": "next build --turbopack",
    "_start": "next start --port $BEEPIT_PORT",
    "start": "dotenv-run-script -- _start",
    "start-forever": "forever start -c 'npm run start' .",
    "stop": "forever stop . && ps -eo pid,pgid,tpgid,args | grep beep && killall -i node && echo 'try kill <pid> if necessary'",
    "lint": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv-run-script": "^0.4.1",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "gray-matter": "^4.0.3",
    "multer": "^2.0.1",
    "next": "15.3.4",
    "pino": "^9.7.0",
    "pts": "^0.11.0",
    "react": "^19.0.0",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.0.0",
    "react-svg": "^16.3.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "ulid": "^3.0.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^1.4.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "9.29.0",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "optionalDependencies": {
    "forever": "^4.0.3",
    "tsx": "^4.20.3"
  }
}
